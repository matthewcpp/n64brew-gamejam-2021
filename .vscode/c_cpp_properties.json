{
    "configurations": [
        {
            "name": "N64",
            "includePath": [
                "/usr/include/n64",
                "/usr/include/n64/nusys",
                "/usr/include/n64/nustd",
                "${workspaceFolder}/lib/framework64/include",
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/build_n64/bin/assets"
            ],
            "defines": [
                "_LANGUAGE_C",
                "PLATFORM_N64"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14"
        },
        {
            "name": "Desktop",
            "includePath": [
                "${workspaceFolder}/lib/framework64/include",
                "${workspaceFolder}/src/**"
            ],
            "defines": [
                "PLATFORM_DESKTOP"
            ],
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "compileCommands": "${workspaceFolder}/build_desktop/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}