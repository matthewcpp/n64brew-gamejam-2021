    "commander": "^6.2.1",
    "fs-extra": "^10.0.0",
    "gl-matrix": "^3.3.0",
    "gltf-import-export": "^1.0.18",
    "jimp": "^0.16.1",
    "opentype.js": "^1.3.3",
    "rimraf": "^3.0.2",
    "sqlite3": "^4.2.0",
    "tmp": "^0.2.1"
  }
}
